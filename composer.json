    }
  ],
  "support": {
    "email": "lucky@t2engine.cn",
    "issues": "https://github.com/scbtl/t2/issues",
    "forum": "https://forum.t2engine.cn/",
    "wiki": "https://doc.t2engine.cn/",
    "source": "https://github.com/scbtl/t2"
  },
  "require": {
    "php": ">=8.3",
    "scbtl/t2-framework": "^1.0.0",
    "monolog/monolog": "^3.8"
  },
  "suggest": {
    "ext-event": "For better performance."
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app"
    },
    "psr-0": {
      "": "extend/"
    }
  },
  "scripts": {
    "post-package-install": [
      "App\\Install::install"
    ],
    "post-package-update": [
      "App\\Install::install"
    ],
    "pre-package-uninstall": [
      "App\\Install::uninstall"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
