  "keywords": [
    "high performance",
    "http service",
    "t2 engine"
  ],
  "homepage": "https://www.t2engine.cn",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "Tony",
      "email": "lucky@t2engine.cn",
      "homepage": "https://www.t2engine.cn",
